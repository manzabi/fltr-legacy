{
    "private": true,
    "name": "fluttr-home",
    "version": "1.0.0",
    "main": "index.js",
    "author": "fluttr",
    "scripts": {
        "dev": "export NODE_ENV=development && export ENV=development && webpack-dev-server --hot --inline --config webpack.dev.js",
        "build": "export NODE_ENV=production && export ENV=production && webpack --mode development --config webpack.prod.js -p",
        "dist": "export NODE_ENV=production && export ENV=production && webpack --mode development --config webpack.prod.js -p",
        "dist-qa": "export NODE_ENV=qa && export ENV=qa && webpack --mode development --config webpack.prod.js -p",
        "dist-stg": "export NODE_ENV=staging && export ENV=staging && webpack --mode development --config webpack.prod.js -p",
        "dist-local": "export NODE_ENV=development && export ENV=development && webpack --mode development --config webpack.prod.js -p",
        "predist": "rm -rf ./build/*",
        "predist-qa": "rm -rf ./build/*",
        "predist-stg": "rm -rf ./build/*",
        "predist-local": "rm -rf ./build/*",
        "serve": "webpack-dev-server --config webpack.prod.js",
        "test": "jest",
        "test:watch": "npm test -- --watch"
    },
